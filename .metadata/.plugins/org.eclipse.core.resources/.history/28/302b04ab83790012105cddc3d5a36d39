package edu.kcg.Poker.Tester;

import edu.kcg.Poker.HandChecker;

/**
 * 役判定モジュールのテスター。
 */
public class HandCheckerTester {
	public static void main(String[] args) {

		// コミュニティカード
		int[] com = { 1, 3, 4, 2, 5 };

		// プレイヤーの手札
		int handl = 51;
		int handr = 50;

		handl <<= 6;

		System.out.println(HandChecker.checkHand(handl | handr, com));

	}
}
